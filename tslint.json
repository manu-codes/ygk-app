{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "no-string-literal": false,
    "interface-name": [true, "never-prefix"],
    "member-access": false,
    "ordered-imports":false
  },
  
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}